{
  "name": "jodid25519",
  "version": "0.7.0",
  "description": "jodid25519 - Curve 25519-based cryptography",
  "main": "index.js",
  "directories": {
    "src": "src",
    "lib": "lib",
    "test": "test",
    "doc": "doc"
  },
  "scripts": {
    "test": "karma start --singleRun=true karma.conf.js --browsers PhantomJS",
    "test-full": "TEST_FULL=true karma start --preprocessors= --singleRun=true karma.conf.js --browsers PhantomJS",
    "test-timing": "TEST_TIMING=true karma start --preprocessors= --singleRun=true karma.conf.js",
    "api-doc": "./node_modules/.bin/jsdoc --destination doc/api/ --private --configure jsdoc.json --recurse src/",
    "build-partial": "make build-test-partial",
    "build-full": "make build-test-full"
  },
  "homepage": "https://github.com/meganz/jodid25519",
  "repository": {
    "type": "git",
    "url": "https://github.com/meganz/jodid25519.git"
  },
  "bugs": {
    "url": "https://github.com/meganz/jodid25519/issues"
  },
  "keywords": [
    "Curve25519",
    "Ed25519",
    "ECDH",
    "EdDSA",
    "ECDSA",
    "encryption",
    "signing"
  ],
  "author": "Michele Bini, Ron Garret, Guy K. Kloss",
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "almond": "~0.2.9",
    "karma-script-launcher": "~0.1.0",
    "karma-chrome-launcher": "~0.1.2",
    "karma-firefox-launcher": "~0.1.3",
    "karma-phantomjs-launcher": "~0.1.1",
    "karma-html2js-preprocessor": "~0.1.0",
    "karma-jasmine": "~0.1.5",
    "requirejs": "~2.1.12",
    "karma-requirejs": "~0.2.2",
    "karma": "~0.10.9",
    "mocha": "~1.17.1",
    "karma-mocha": "~0.1.1",
    "karma-chai": "0.1.0",
    "karma-chai-plugins": "0.2.1",
    "karma-sinon": "~1.0.3",
    "karma-coverage": "~0.2.4",
    "istanbul": "~0.2.10",
    "ibrik": "~1.1.1",
    "dateformat": "~1.0.7-1.2.3",
    "sinon": "~1.9.0",
    "jsdoc": "~3.3.0"
  }
}
